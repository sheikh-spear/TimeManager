m class=\"form\" v-on:submit.prevent=\"onSubmit\">\n            <h2 class=\"form__title\">Join thousand of worker from around the world</h2>\n            <p class=\"form__text\">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Quisque a dui at dui bibendum mattis. Aenean ultrices ligula vitae rutrum mattis.</p>\n            <label class=\"form__label\" for=\"email\">Email</label>\n            <input v-model=\"email\" placeholder=\"Email\" class=\"form__input\" type=\"email\" name=\"email\" id=\"email\" required>\n            <label class=\"form__label\" for=\"password\">Password</label>\n            <input v-model=\"password\" placeholder=\"Password\" class=\"form__input\" type=\"password\" id=\"password\" name=\"password\" required>\n            <input class=\"form__button\" type=\"submit\" value=\"Start to work now\">\n            <div class=\"error\">\n                <p>{{errorMessage}}</p>\n            </div>\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  methods: {\n    getAuthenticatedToken: function() {\n      //Send request to API to get the user web token\n      axios.post(\"/api/users/sign_in\").then(\n        response => {\n          console.log(response.data);\n          //If success, redirect to /\n          this.$router.push(\"/\");\n        },\n        error => {\n          //If error\n          console.log(error.response.data.errors.detail);\n          console.log(this);\n          this.errorMessage = error.response.data.errors.detail;\n        }\n      );\n    }\n  },\n  data() {\n    return {\n      email: null,\n      password: null,\n      errorMessage: null,\n      onSubmit: function() {\n        this.getAuthenticatedToken();\n      }\n    };\n  }\n};\n</script>\n\n<style lang=\"scss\">\n.login {\n  overflow: auto;\n  height: 100vh;\n  width: 100vw;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.form {\n  border: 1px solid #bdbdbd;\n  box-sizing: border-box;\n  border-radius: 24px;\n  width: 50%;\n  color: #333333;\n  padding: 90px 60px;\n\n  .error {\n    color: darkred;\n    padding: 20px 0;\n  }\n\n  &__title {\n    font-style: normal;\n    font-weight: 600;\n    font-size: 18px;\n    line-height: 25px;\n    margin-bottom: 20px;\n  }\n\n  &__text {\n    font-style: normal;\n    font-weight: normal;\n    font-size: 16px;\n    line-height: 22px;\n    margin-bottom: 40px;\n  }\n\n  &__label {\n    display: none;\n  }\n\n  &__input {\n    padding: 16px;\n    box-sizing: border-box;\n    border-radius: 8px;\n    border: 1px solid #bdbdbd;\n    font-style: normal;\n    font-weight: normal;\n    font-size: 16px;\n    line-height: 22px;\n    margin-bottom: 14px;\n    display: block;\n    width: 100%;\n  }\n\n  &__button {\n    background: #2f80ed;\n    border-radius: 8px;\n    display: block;\n    font-style: normal;\n    font-weight: 600;\n    font-size: 16px;\n    line-height: 22px;\n    text-align: center;\n    color: #ffffff;\n    padding: 10px 120px;\n    width: 100%;\n    border: none;\n    cursor: pointer;\n    margin-top: 30px;\n  }\n}\n</style>\n\n\n"],"sourceRoot":"src/components"}]}